{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es5",
      "dom"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "cypress",
      "node"
    ],
    "typeRoots": [
      "./cypress/@types",
      "./node_modules/@types"
    ]
  },
  "include": [
    "**/*.ts"
  ]
}
